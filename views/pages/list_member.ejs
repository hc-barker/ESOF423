<!DOCTYPE html>
<head>
	<% include ../partials/head.ejs %>
</head>

<header>
	<% include ../partials/header.ejs %>
</header>

<main>
<div class='container'>
	<h2>List all senior center memers</h2>
	<table class='table table-bordered'>
		<thead>
			<th style='display:none'>ID</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Birth Date</th>
			<th>Activities</th>
		</thead>
		<tbody>
			<% for(var i=0; i<members.length; i++) {%>
			<tr>
				<td style='display:none'><%= members[i]._id %></td>
				<td><%= members[i].firstname %></td>
				<td><%= members[i].lastname %></td>
				<td><%= members[i].birthdate %></td>
				<td><%= members[i].activities %></td>
			</tr>
			<% } %>
		</tbody>
	</table>
</div>
</main>

<footer>
	<% include ../partials/footer.ejs %>
</footer>

</body>
</html>
